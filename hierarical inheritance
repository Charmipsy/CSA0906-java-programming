import java.io.*;
import java.util.*;
class P
{
int x=10;
void display()
{
System.out.println(x);
}
}
class C1 extends P
{
int y=5;
void display1()
{
System.out.println(y);
}
}
class C2 extends P
{
int z=5;
void display2()
{
System.out.println(+z);
}
}
class hierarcial
{
public static void main(String args[])
{
C2 a=new C2();
a.display2();
a.display();
C1 b=new C1();
b.display1();
}
}
