import java.io.*;
import java.util.*;
class GP
{
int x=10;
void display()
{
System.out.println("am having 10cr"+x);
}
}
class P extends GP
{
int y=10;
void display1()
{
System.out.println("i also have 10cr"+y);
}
}
class C extends P
{
int z=5;
void display2()
{
System.out.println("Z="+z);
}
}
class multilevel
{
public static void main(String args[])
{
C a=new C();
a.display2();
a.display1();
a.display();
}
}
